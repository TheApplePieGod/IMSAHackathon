{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "es2020",
      "dom"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "Backend/*.ts",
    "Backend/*.tsx"
  ]
}
